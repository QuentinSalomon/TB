<UserControl x:Class="Framework.UserControlEditPartition"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Framework"
             xmlns:concept="http://www.objectis.ch/concept"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400" MouseWheel="UserControl_MouseWheel">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>

        <Rectangle x:Name="RectangleViewNotes" Fill="White"/>
        <StackPanel x:Name="StackPanelNotesPitch" Grid.Column="1"/>
    </Grid>
</UserControl>
